    <script src="{{ asset('js/jquery.form.js') }}"  type="text/javascript"></script>
  	<script src="/js/superscrollorama/js/jquery.lettering-0.6.1.min.js"></script>
	<script src="/js/superscrollorama/js/jquery.superscrollorama.js"></script>


    
